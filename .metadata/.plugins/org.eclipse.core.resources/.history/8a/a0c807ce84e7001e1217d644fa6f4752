
public class Number_To_String 
{

	String[] units = {"", "one", "two", "three", "four", "five", "six", "seven", "eight", "nine"};
    String[] teens = {"", "eleven", "twelve", "thirteen", "fourteen", "fifteen", "sixteen", "seventeen", "eighteen", "nineteen"};
    String[] tens = {"", "ten", "twenty", "thirty", "forty", "fifty", "sixty", "seventy", "eighty", "ninety"};
    
    
    
    public static void main(String[] args) {
    
    int num = 23;	
    String ss = Convert_to_String(num);
    System.out.println(num + " = " + ss);
    
    }
	
    
    
    public static String Convert_to_String(int val)
    {
    	String word = "";
    	
    	if ((val/100000) > 0)
    	{
    		word += Convert_to_String(val/100000) + "Lakh";
    		val %= 100000;
    	}
    	
    	if ((val/1000) > 0)
    	{
    		word += Convert_to_String(val/100000) + "Thousand";
    		val %= 100000;
    	}
    	
    	if ((val/100) > 0)
    	{
    		word += Convert_to_String(val/100000) + "Hundred";
    		val %= 100000;
    	}
    	
    	return "";
    }
    
    
	
}
