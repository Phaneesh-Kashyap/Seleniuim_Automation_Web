package Programs;

public class Palindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int val1 = 121;
		boolean val = isPalindrome(val1);
		
	}

	public static boolean isPalindrome(int x) {
		int y=0;
		
		while (x >0){
				y = y*10 +(x%10);
				x=x/10;
			
		}
		
		return true;
    }
	
}
