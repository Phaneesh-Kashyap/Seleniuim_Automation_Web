package Programs;

public class Search_Insert_Position {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] nums = {1,3,5,6};
		int target = 7;
		
		int[] nums1 = plusOne(nums);
		 for(int i=0; i<nums1.length; i++)
		  {
			 System.out.println(nums1[i]);
			  
		  }
		
		
			String s="Hello World";
		  String[] litrals = s.split(" ");
		  //int len = litrals.length;
		  System.out.println(litrals[litrals.length-1].length());
		  //int len = litrals.length;
		  
		  
		
	 	
	    int ss = searchInsert(nums,target);
	    System.out.println(ss);

	}

	
	  public static int searchInsert(int[] nums, int target) {
		  int i;
		  
		  
		  for (i=0; i<nums.length;i++)
		  {
			  if (nums[i]>=target)
			  {
				  return i;
			  }
		  }
		return i;
		
		
		
		    
	    }
	  
	  
	  public static int[] plusOne(int[] digits) 
	  {
		  int val = 0;
		  for(int i=0; i<digits.length; i++)
		  {
			  val=val*10+digits[i];
		  }
		  val++;
		  String val2 = Integer.toString(val);
		  int[] newdigit = new int[val2.length()];
		  for(int i=0; i<val2.length() ;i++)
		  {
			  newdigit[i]=(int)val2.charAt(i);
			  
		  }
		  
		  return newdigit;
	     //   for(int i;i<)
	    }


}
